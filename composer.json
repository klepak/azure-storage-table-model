{
    "name": "klepak/azure-storage-table-model",
    "description": "Provides a model wrapper around the Azure Storage Table API",
    "authors": [
        {
            "name": "Knut Leborg",
            "email": "knut@lepa.no"
        }
    ],
    "minimum-stability": "dev",

    "require": {
        "laravel/framework": "^5.6",
        "microsoft/azure-storage-table": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*",
        "orchestra/testbench": "~3.0",
        "vlucas/phpdotenv": "^2.5@dev"
    },
    "autoload": {
        "psr-4": {
            "Klepak\\AstModel\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Klepak\\AstModel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Klepak\\AstModel\\AstModelServiceProvider"
            ]
        }
    }
}
